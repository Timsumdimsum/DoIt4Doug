<Window x:Class="DoIt4Doug.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DoIt4Doug"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="getDataBtn" Content="Button" Grid.Column="2" HorizontalAlignment="Left" Margin="37,128,0,0" VerticalAlignment="Top" Width="74" Click="getDataBtn_Click" Height="20"/>
        <TextBox x:Name="ipAddressTxtBx" Grid.Column="2" HorizontalAlignment="Left" Height="22" Margin="37,74,0,0" TextWrapping="Wrap" Text="192.168.0.10" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
